+float BaseAudioContext::JitterAudioData(float data, ExecutionContext* context) {
+  if (!base::FeatureList::IsEnabled(blink::features::kHeliumNoiseAudio)) {
+    return data;
+  }
+
+  if (!context || !context->GetHeliumNoiseTokens().contains(
+      HeliumNoiseFeature::kAudio)) {
+    return data;
+  }
+
+  HeliumNoiseHash noise_hash(
+      context->GetHeliumNoiseTokens().at(HeliumNoiseFeature::kAudio));
+
+  // Update the hash with audio data for extra entropy. This makes it
+  // harder to identify or remove the noise, similar to how we do
+  // it in canvas.
+  uint64_t data_bits = std::bit_cast<uint64_t>(static_cast<double>(data));
+  noise_hash.Update(data_bits);
+
+  // Generate jitter within [-0.0001, 0.0001] (aka Â±0.01%)
+  // Note: GetValueBelow(2) returns an int value, either 0 or 1.
+  constexpr float scale = 0.0001f / 10000.0f;
+  int noise_value = noise_hash.GetValueBelow(10000 + 1);
+  int noise_sign = noise_hash.GetValueBelow(2) ? 1 : -1;
+  float jitter = noise_value * noise_sign * scale;
+
+  return data * (1.0f + jitter);
+}
